{
  "HashiCorpVaultConfig" : {
    "Host" : "https://vault.easy-auth.ru",
    "MountPoint": "secret/",
    "RootPath" : "easy-quiz/prod"
  },
  "Authority-secret": "auth/authority",
  "AllowedHosts": "*",
  "ClusterConfig": {
    "ConnectionString-secret": "cluster/client1/host",
    "ClusterId-secret": "cluster/client1/cluster-id",
    "ServiceId-secret": "cluster/client1/service-id"
  },
  "GptServiceSettings": {
    "AuthUrl-secret": "cluster/client1/gpt-auth-url",
    "Url-secret": "cluster/client1/gpt-url",
    "ClientSecret-secret": "cluster/client1/gpt-client-secret",
    "Scope-secret": "cluster/client1/gpt-scope"
  },
  "Kestrel": {
    "Endpoints" : {
      "Http" : {
        "Url" : "http://0.0.0.0:5530",
        "Protocols": "Http1AndHttp2"
      },
      "Https": {
        "Url": "https://localhost:5531",
        "Protocols": "Http2"
      }
    }
  }
}

